<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{{title}}</title>
    <link rel="stylesheet" href="css/style.css">
  </head>
  <body>
    {{>header}}
    <div class="app-main">
    
      <div class="inputsearchBox">
         
       <input type="text" id="input-box" class="input-box" placeholder="search..." autocomplete="off">
         
    </div>

    
      <div class="weather-body">
            <div class="location-details">
                 <div class="city" id="city">place</div>
                 <div class="date" id="date" >10 June (Wednesday), 2022</div>
            </div>
            
            <div class="weather-status">
                <div class="temp" id="temp">Temp</div>
                <div class="min-max" id="MinMax">Min Temp | Max Temp</div>
                <div class="weather" id="weather">weather</div>
                <div class="img"></div>
            </div>


          </div>

    </div>
    <script src="js/app.js"></script>
  </body>
</html>